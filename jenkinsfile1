pipeline
{
agent any
stages
 {
    stage ("scm checkout")
    {
            steps{git branch: 'master', url: 'https://github.com/Devopstech321/mavenproject.git'}
        }

    stage ("test")
    {
        steps {withMaven(globalMavenSettingsConfig: '', jdk: 'JAVA-HOME', maven: 'MAVN-HOME') {
    sh 'mvn test'
}}    }

    stage ("artifact created")
    {
        steps {withMaven(globalMavenSettingsConfig: '', jdk: 'JAVA-HOME', maven: 'MAVN-HOME') {
    sh 'mvn package'
}}    }
    
 }
}